# BROADCAST MODULE CONFIGURATION
# Copy this to .env and configure with your ZAPI credentials

# ZAPI Configuration
ZAPI_API_URL=https://api.z-api.io
ZAPI_TOKEN=your-zapi-token-here
ZAPI_INSTANCE_ID=your-instance-id-here

# Broadcast Module Settings
BROADCAST_MODULE_ENABLED=true
BROADCAST_API_KEY=your-broadcast-api-key-here

# Rate Limiting
BROADCAST_RATE_LIMIT_REQUESTS=30
BROADCAST_RATE_LIMIT_WINDOW=60000

# Core Module (existing)
CORE_MODULE_ENABLED=true
EVOLUTION_API_URL=http://your-evolution-api-url:8080
EVOLUTION_API_KEY=your-evolution-api-key
EVOLUTION_INSTANCE_NAME=your-instance-name

# Database (existing)
NODE_ENV=production
APP_PORT=3000
SKIP_DB=false